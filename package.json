{
  "name": "renoirb.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt-ts build",
    "commit": "npx git-cz",
    "dev": "nuxt-ts",
    "fix": "use-run-all fix:*",
    "fix:prettier": "use-cross-env conventions-code-formatter prettier '**/*.{vue,scss,js,ts,json,md}' --write",
    "fix:sort-package-json": "use-cross-env conventions-code-formatter sort-package-json",
    "generate": "nuxt-ts generate",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Dependencies must be installed with Yarn: https://yarnpkg.com/')\"",
    "lint": "use-cross-env use-run-all lint:* fix",
    "lint:js": "use-cross-env conventions-use-eslint --fix --ext .ts,.tsx,.js,.vue --ignore-path .gitignore .",
    "lint:style": "use-cross-env stylelint **/*.{vue,css} --ignore-path .gitignore",
    "start": "nuxt-ts start",
    "test": "use-cross-env jest --detectOpenHandles",
    "test:snapshots": "use-cross-env jest --detectOpenHandles -u"
  },
  "dependencies": {
    "@nuxt/content": "^1.5.0",
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/composition-api": "^0.12.3",
    "@nuxtjs/feed": "^2.0.0",
    "@nuxtjs/pwa": "^3.0.0",
    "nuxt": "^2.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/prompt-cli": "^9.1.1",
    "@nuxt/types": "^2.14.0",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@renoirb/conventions-code-formatter": "^1.3.0",
    "@renoirb/conventions-use-eslint": "^1.2.1",
    "@renoirb/conventions-use-typescript-3": "^1.2.1",
    "@renoirb/tools-bundling-helpers": "^1.2.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^12.12.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "commitlint-config-cz": "^0.13.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "proposal-temporal": "^0.3.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.1.3",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "vue": "^2.6.0",
    "vue-jest": "^3.0.4"
  }
}
